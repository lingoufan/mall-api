<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.liekai.mall.api.mapper.MallUserMapper">

    <resultMap id="BaseResultMap" type="top.liekai.mall.api.entity.MallUser">
            <id property="userId" column="user_id" jdbcType="BIGINT"/>
            <result property="nickName" column="nick_name" jdbcType="VARCHAR"/>
            <result property="loginName" column="login_name" jdbcType="VARCHAR"/>
            <result property="passwordMd5" column="password_md5" jdbcType="VARCHAR"/>
            <result property="introduceSign" column="introduce_sign" jdbcType="VARCHAR"/>
            <result property="address" column="address" jdbcType="VARCHAR"/>
            <result property="isDeleted" column="is_deleted" jdbcType="TINYINT"/>
            <result property="lockedFlag" column="locked_flag" jdbcType="TINYINT"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">

        user_id,nick_name,login_name,

        password_md5,introduce_sign,address,

        is_deleted,locked_flag,create_time

    </sql>
    <insert id="insert"></insert>
    <insert id="insertSelective"></insert>
    <update id="updateByPrimaryKey"></update>
    <update id="updateByPrimaryKeySelective"></update>
    <delete id="deleteByPrimaryKey"></delete>


    <select id="selectByPrimaryKey" parameterType="java.lang.Long" resultMap="BaseResultMap">

        select

        <include refid="Base_Column_List" />

        from tb_mall_user

        where  user_id = #{userId,jdbcType=BIGINT}

    </select>

    <select id="selectAll" resultType="top.liekai.mall.api.entity.MallUser">

        select *from tb_mall_user;

    </select>

</mapper>
